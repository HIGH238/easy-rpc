{
    "name": "easy-rpc",
    "version": "1.2.1",
    "description": "A easy discord rpc",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "build": "pkg . --output build/easyRPC"
    },
    "keywords": [
        "Discord",
        "RPC"
    ],
    "author": "Amitoj Singh",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.19.0",
        "discord-rich-presence": "0.0.8",
        "ejs": "^3.1.5",
        "express": "^4.17.1",
        "multer": "^1.4.2"
    },
    "pkg": {
        "assets": [
            "node_modules/**/*",
            "static/**/*",
            "views/**/*"
        ],
        "targets": [
            "node14-macos-x64",
            "node14-win-x64",
            "node14-linux-x64"
        ]
    },
    
    "bin": "index.js",
    "devDependencies": {
        "pkg": "^4.5.0",
        "nodemon": "^2.0.7"
    }
}
